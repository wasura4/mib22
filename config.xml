<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wasu.mib1" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MIB Brunei</name>
    <description>
        MIB with Push Notifications.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        KPGS Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <gap:plugin name="pushbots-cordova-plugin" spec="^1.4.0" source="npm" />
<preference name="phonegap-version" value="cli-6.3.0" />
<preference name="android-build-tool" value="gradle" />
    <icon src="www/index.hyperesources/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
